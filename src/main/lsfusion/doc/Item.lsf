//#module
MODULE Item;
//#module end

//#class
CLASS Item 'Товар';

name 'Наименование' = DATA VARSTRING[100](Item) IN base;
barcode 'Штрихкод' = DATA STRING[13](Item) IN base;
//#class end

//#price
salePrice 'Оптовая цена' = DATA NUMERIC[17,2](Item) IN base;
//#price end

//#form
FORM items 'Товары'
	OBJECTS i=Item
	PROPERTIES(i) name, barcode, salePrice, NEW, DELETE
;
//#form end